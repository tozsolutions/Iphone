{
  "name": "iphone-app",
  "version": "1.0.0",
  "description": "A modern, responsive web application optimized for iPhone devices",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node src/server.js",
    "build": "npm run lint && npm run test",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "docker:build": "docker build -t iphone-app .",
    "docker:run": "docker run -p 3000:3000 iphone-app",
    "netlify:dev": "npx netlify-cli dev"
  },
  "keywords": [
    "iphone",
    "mobile",
    "responsive",
    "web-app",
    "pwa",
    "production-ready"
  ],
  "author": "tozsolutions",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "@eslint/js": "^9.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tozsolutions/Iphone.git"
  },
  "bugs": {
    "url": "https://github.com/tozsolutions/Iphone/issues"
  },
  "homepage": "https://github.com/tozsolutions/Iphone#readme"
}